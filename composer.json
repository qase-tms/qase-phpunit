{
  "name": "qase/phpunit-reporter",
  "description": "Qase PHPUnit reporter",
  "homepage": "https://qase.io",
  "license": "Apache-2.0",
  "type": "library",
  "keywords": [
    "qase",
    "tms",
    "reporter",
    "phpunit",
    "common",
    "php",
    "unit"
  ],
  "authors": [
    {
      "name": "Qase Team",
      "email": "support@qase.io"
    }
  ],
  "require": {
    "php": "^8.0",
    "phpunit/phpunit": "^9 || ^10 || ^11",
    "qase/php-commons": "^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Qase\\PHPUnitReporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "version": "2.0.1",
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
