{
  "name": "qase/phpunit-reporter",
  "description": "Qase TMS PHPUnit reporter.",
  "homepage": "https://developers.qase.io",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Kamil Samigullin",
      "email": "kamil@samigullin.info"
    },
    {
      "name": "Sergey Skaridov",
      "email": "s.skaridov@gmail.com"
    }
  ],
  "keywords": [
    "qase",
    "tms",
    "reporter",
    "php",
    "phpunit"
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "phpunit/phpunit": "^8.0 || ^9.0",
    "qase/api": "v1.0.0-alpha.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "Qase\\PHPUnit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
